<classifier-config xmlns="http://epicenter.hmsinc.com/classifier">

	<name>TestClassifier</name>
	<description>Simple classifier for testing purposes.</description>
	<version>1.0</version>
	<implementation>com.hmsinc.epicenter.classifier.lm.BestCategoryClassifier</implementation>

	<training-set>
	
		<category name="Pass">
			<entry value="Pass" />
		</category>
		
		<category name="Fail">
			<entry value="Fail" />
		</category>
		
	</training-set>

</classifier-config>

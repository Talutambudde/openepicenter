<classifier xmlns="http://epicenter.hmsinc.com/model">

	<name>TestClassifier</name>
	<description>Simple classifier for testing purposes.</description>
	<version>1.0</version>
	<type>STANDARD</type>
	
	<targets>
		<target>
			<datatype>com.hmsinc.epicenter.model.health.Registration</datatype>
			<interactiontype>EMERGENCY_DEPT</interactiontype>
			<property>reason</property>
		</target>
	</targets>
	
	<categories>
	
		<category name="Pass">
			<entry>Pass</entry>
			<map>HI THERE</map>
		</category>
		
		<category name="Fail">
			<entry>Fail</entry>
		</category>
		
	</categories>

</classifier>

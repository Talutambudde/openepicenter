<attributes xmlns="http://epicenter.hmsinc.com/model">

	<agegroups>
	
		<!-- New age groups to replace pediatric TRX-143 -->
		<agegroup name="Unknown" />
		<agegroup name="Infant" minimumAge="0" maximumAge="2" />
		<agegroup name="Preschool" minimumAge="3" maximumAge="5" />
		<agegroup name="Child" minimumAge="6" maximumAge="12" />		
		<agegroup name="Adolescent" minimumAge="13" maximumAge="17" />
		<agegroup name="Adult" minimumAge="18" maximumAge="64" />
		<agegroup name="Geriatric" minimumAge="65" maximumAge="150" />

	</agegroups>

	<genders>
		
		<gender name="Unknown" abbreviation="U" />
		<gender name="Male" abbreviation="M" />
		<gender name="Female" abbreviation="F" />

	</genders>

	<patient-classes>
		<patient-class name="Outpatient" abbreviation="O" />
		<patient-class name="Inpatient" abbreviation="I" />
		<patient-class name="Emergency Department" abbreviation="E" />
	</patient-classes>

</attributes>


